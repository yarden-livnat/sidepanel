{
  "name": "sidepanel",
  "version": "0.2.0",
  "description": "A sidepanel output widget for JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab extension",
    "widgets"
  ],
  "homepage": "https://github.com/yarden-livnat/sidepanel",
  "bugs": {
    "url": "https://github.com/yarden-livnat/sidepanel/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/yarden-livnat/sidepanel"
  },
  "scripts": {
    "build": "npm run build:lib",
    "build:labextension": "cd sidepanel && rimraf labextension && mkdirp labextension && cd labextension && npm pack ../..",
    "build:lib": "tsc",
    "build:all": "npm run build:lib && npm run build:labextension",
    "clean": "npm run clean:lib",
    "clean:lib": "rimraf lib",
    "clean:labextension": "rimraf sidepanel/labextension",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^2.0.1",
    "@jupyter-widgets/jupyterlab-manager": "^1.0.1",
    "@jupyterlab/application": "^1.0.1",
    "@phosphor/application": "^1.6.3",
    "@phosphor/widgets": "^1.8.1"
  },
  "devDependencies": {
    "@types/node": "^12.6.2",
    "fs-extra": "^8.1.0",
    "json-loader": "^0.5.7",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.3"
  },
  "jupyterlab": {
    "extension": "lib/plugin"
  }
}
