{
  "compilerOptions": {
    "declaration": true,
    "noImplicitAny": false,
    "lib": ["dom", "es6"],
    "noEmitOnError": false,
    "strictNullChecks": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2016",
    "outDir": "../lib",
    "skipLibCheck": true,
    "sourceMap": true,
    "types": ["node"]
  },
  "include": [
    "src/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
